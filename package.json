        "morgan": "^1.10.0",
        "pug": "^3.0.2"
    },
    "devDependencies": {
        "@prettier/plugin-pug": "^2.0.0",
        "autoprefixer": "^10.4.7",
        "babelify": "^10.0.0",
        "browser-sync": "^2.27.10",
        "browserify": "^17.0.0",
        "cssnano": "^5.1.8",
        "del": "^6.1.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.2.0",
        "eslint-plugin-promise": "^6.0.0",
