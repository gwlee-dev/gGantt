        "gulp-sass": "^5.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-terser": "^2.1.0",
        "gulp-util": "^3.0.8",
        "nodemon": "^2.0.16",
        "postcss": "^8.4.14",
        "prettier": "^2.6.2",
        "sass": "^1.51.0",
        "stylelint": "^14.8.5",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-recommended-scss": "^6.0.0",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    }
