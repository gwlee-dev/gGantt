    "main": "index.js",
    "scripts": {
        "zip": "export DEST=dist/static && babel-node ./node_modules/.bin/gulp compress",
        "build": "export DEST=dist/static && babel-node ./node_modules/.bin/gulp compress",
        "dev:build": "export DEST=client && babel-node ./node_modules/.bin/gulp build",
        "server": "export DEST=client && nodemon --watch ./server --exec babel-node ./server/init.js",
        "watch": "npm run dev:build && babel-node ./node_modules/.bin/gulp dev",
        "eslint": "npx eslint ./ --fix",
        "stylelint": "npx stylelint ./**/*.scss --fix"
    },
    "dependencies": {
        "@babel/core": "^7.17.12",
        "@babel/node": "^7.17.10",
        "@babel/preset-env": "^7.17.12",
        "dotenv": "^16.0.1",
